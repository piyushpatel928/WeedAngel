            -webkit-background-clip: padding-box;
            -moz-background-clip: padding;
            background-clip: padding-box;
        }

            .dropdown-menuweed.pull-right {
                right: 0;
                left: auto;
            }

            .dropdown-menuweed .divider {
                *width: 100%;
                height: 1px;
                margin: 9px 1px;
                *margin: -5px 0 5px;
                overflow: hidden;
                background-color: #e5e5e5;
                border-bottom: 1px solid #fff;
            }

            .dropdown-menuweed > li > a {
                display: block;
                padding: 3px 20px;
                clear: both;
                font-weight: normal;
                line-height: 20px;
                color: #333;
                white-space: nowrap;
            }

                .dropdown-menuweed > li > a:hover, .dropdown-menuweed > li > a:focus, .dropdown-submenu:hover > a, .dropdown-submenu:focus > a {
                    text-decoration: none;
                    color: #fff;
                    background-color: #f56201;
                    background-image: -moz-linear-gradient(top, #ff6702, #e85c00);
                    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#ff6702), to(#e85c00));
                    background-image: -webkit-linear-gradient(top, #ff6702, #e85c00);
                    background-image: -o-linear-gradient(top, #ff6702, #e85c00);
                    background-image: linear-gradient(to bottom, #ff6702,#e85c00);
                    background-repeat: repeat-x;
                    filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#FFFF6702, endColorstr=#FFE85C00, GradientType=0);
                }

            .dropdown-menuweed > .active > a, .dropdown-menuweed > .active > a:hover, .dropdown-menuweed > .active > a:focus {
                color: #fff;
                text-decoration: none;
                outline: 0;
                background-color: #f56201;
                background-image: -moz-linear-gradient(top, #ff6702, #e85c00);
                background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#ff6702), to(#e85c00));
                background-image: -webkit-linear-gradient(top, #ff6702, #e85c00);
                background-image: -o-linear-gradient(top, #ff6702, #e85c00);
                background-image: linear-gradient(to bottom, #ff6702,#e85c00);
                background-repeat: repeat-x;
                filter: progid:DXImageTransform.Microsoft.gradient(startColorstr=#FFFF6702, endColorstr=#FFE85C00, GradientType=0);
            }

            .dropdown-menuweed > .disabled > a, .dropdown-menuweed > .disabled > a:hover, .dropdown-menuweed > .disabled > a:focus {
                color: #999;
            }

                .dropdown-menuweed > .disabled > a:hover, .dropdown-menuweed > .disabled > a:focus {
                    text-decoration: none;
                    background-color: transparent;
                    background-image: none;
                    filter: progid:DXImageTransform.Microsoft.gradient(enabled = false);
                    cursor: default;
                }

        .open {
            *z-index: 1000;
        }

            .open > .dropdown-menuweed {
                display: block;
            }

        .pull-right > .dropdown-menuweed {
            right: 0;
            left: auto;
        }

        .dropup .caret, .navbar-fixed-bottom .dropdown .caret {
            border-top: 0;
            border-bottom: 4px solid #000;
            content: "";
        }

        .dropup .dropdown-menuweed, .navbar-fixed-bottom .dropdown .dropdown-menuweed {
            top: auto;
            bottom: 100%;
            margin-bottom: 1px;
        }

        .dropdown-submenu {
            position: relative;
        }

            .dropdown-submenu > .dropdown-menuweed {
                top: 0;
                left: 100%;
                margin-top: -6px;
                margin-left: -1px;
                -webkit-border-radius: 0 6px 6px 6px;
                -moz-border-radius: 0 6px 6px 6px;
                border-radius: 0 6px 6px 6px;
            }

            .dropdown-submenu:hover > .dropdown-menuweed {
                display: block;
            }

        .dropup .dropdown-submenu > .dropdown-menuweed {
            top: auto;
            bottom: 0;
            margin-top: 0;
            margin-bottom: -2px;
            -webkit-border-radius: 5px 5px 5px 0;
            -moz-border-radius: 5px 5px 5px 0;
            border-radius: 5px 5px 5px 0;
        }

        .dropdown-submenu > a:after {
            display: block;
            content: " ";
            float: right;
            width: 0;
            height: 0;
            border-color: transparent;
            border-style: solid;
            border-width: 5px 0 5px 5px;
            border-left-color: #ccc;
            margin-top: 5px;
            margin-right: -10px;
        }

        .dropdown-submenu:hover > a:after {
            border-left-color: #fff;
        }

        .dropdown-submenu.pull-left {
            float: none;
        }

            .dropdown-submenu.pull-left > .dropdown-menuweed {
                left: -100%;
                margin-left: 10px;
                -webkit-border-radius: 6px 0 6px 6px;
                -moz-border-radius: 6px 0 6px 6px;
                border-radius: 6px 0 6px 6px;
            }

